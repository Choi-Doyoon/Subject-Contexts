#include<iostream>
using namespace std;

int main(){
    char tree[] = {
        'A',
        'B','C',
        'D','E','F', NULL,
        'G'
    };

    for(int i=0;i<8;i++){
        if(tree[i] == NULL){continue;}

        cout<<tree[i]<<"의 자식: ";
        if(2*i+1 < 8 && tree[2*i+1] != NULL){
            cout<<tree[2*i+1]<<" ";
        }
        if(2*i+2 < 8 && tree[2*i+2] != NULL){
            cout<<tree[2*i+2]<<" ";
        }
        cout<<endl;
    }
    cout<<endl;

    for(int i=1;i<8;i++){
        if(tree[i]==NULL){continue;}
        cout<<tree[i]<<"의 부모: "<<tree[(int)((i-1)/2)]<<endl;
    }
}